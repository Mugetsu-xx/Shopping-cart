{{#each products}}
<div class="row">
  {{#each this}}
  <div class="col-sm-6 col-md-4">
    <div class="card" style="width: 18rem;">
      <img src="{{this.imagePath}}" class="card-img-top image-responsive" alt="...">
      <div class="card-body">
        <h5 class="card-title">{{this.title}}</h5>
        <p class="card-text ">{{this.description}}</p>
        <div>
          <div class="price">${{this.price}}</div>
          <a href="#" class="btn btn-success nav navbar pull-right">Add To Cart</a>
        </div>
      </div>
    </div>
  </div>
  {{/each}}
</div>
{{/each}}